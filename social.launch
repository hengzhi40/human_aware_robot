<launch>

	<node pkg="turtlesim" type="turtlesim_node" name="sim"/>

	<node name="human_turtle" pkg="human_aware_robot" type="human-turtle.py" respawn="false" output="screen">
		<param name="turtle" type="string" value="turtle1"/>
	</node>

	<node name="robot_turtle" pkg="human_aware_robot" type="human-turtle.py" output="screen">
		<param name="turtle" type="string" value="turtle2" />
	</node>

	<node name="listener" pkg="human_aware_robot" type="robot-turtle.py" />

</launch>